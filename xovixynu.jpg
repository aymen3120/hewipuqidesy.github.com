  function geoCodeViaGoogle(addy, state) {
    //var theGeoCall = "http://maps.googleapis.com/maps/api/geocode/json?address=" + addy + "&sensor=false";
  
    var geocoder = new google.maps.Geocoder();
    var theLatLng = "";
  var coordArray = [];
    geocoder.geocode( { 'address': addy}, function(results, status) {
      if (status == google.maps.GeocoderStatus.OK) {
        theLatLng = results[0].geometry.location;
       
        coordArray[0] = theLatLng.lat();
        
        coordArray[1] = theLatLng.lng();
        
       
        
        window.location = "/q/addy-" + state + "-restaurants/?lat=" + coordArray[0] + "&lon=" + coordArray[1] + "&rad=50";
        
      } else {
        alert(addy);
        alert("Geocode was not successful for the following reason: " + status);
      }
    });

}  </script>
