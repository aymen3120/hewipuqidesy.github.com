      <script type="text/javascript">
    function geoCodeTheSearchViaGoogle() {
    //var theGeoCall = "http://maps.googleapis.com/maps/api/geocode/json?address=" + addy + "&sensor=false";
  
    var geocoder = new google.maps.Geocoder();
    var theWhere = $("#txtLocoSearch").val();
    var theWhat = $("#txtWhatSearch").val();
    var theLatLng = "";
  var coordArray = [];
    geocoder.geocode( { 'address': theWhere}, function(results, status) {
      if (status == google.maps.GeocoderStatus.OK) {
        theLatLng = results[0].geometry.location;
       
        coordArray[0] = theLatLng.lat();
        
        coordArray[1] = theLatLng.lng();
        
        
var n = theWhat.indexOf('\'');
theWhat = theWhat.substring(0, n != -1 ? n : theWhat.length);
                       
                                                    
         theWhat = theWhat.replace(/ /g, "-");
      theWhat = theWhat.replace(/,/g,"");
     //theWhat = theWhat.replace(/'/g,"");
      theWhere = theWhere.replace(/ /g, "-");
      theWhere = theWhere.replace(/,/g,"-");
      theWhere = theWhere.replace(/'/g,"");
      theWhere = theWhere.replace(/--/g,"-");
        if((theWhat == "") || (theWhat == undefined)) {
          window.location = "/q/" + theWhere + "-restaurants/?lat=" + coordArray[0] + "&lon=" + coordArray[1] + "&rad=50";            
        } else {
          window.location = "/q/" + theWhere + "-restaurants/" + theWhat + "?lat=" + coordArray[0] + "&lon=" + coordArray[1] + "&rad=50";     
        }
      } else {
        alert(theWhere);
        alert("Geocode was not successful for the following reason: " + status);
      }
    });

}     
  </script>




  <script type="text/javascript">
    function doSearch() {
                         //alert("test");
      var theWhat = $("#txtWhatSearch").val();
      var theWhere = $("#txtLocoSearch").val();
      
      theWhat = theWhat.replace(/ /g, "-");
      theWhat = theWhat.replace(/,/g,"");
      theWhat = theWhat.replace(/'/g,"");
      theWhere = theWhere.replace(/ /g, "-");
      theWhere = theWhere.replace(/,/g,"-");
      theWhere = theWhere.replace(/'/g,"");
      theWhere = theWhere.replace(/--/g,"-");
      
      var theURL = "";
                         
    var position= theWhat.indexOf("'",0);       
    if(position != -1) {
       theWhat = theWhat.substring(0,position);
                       
    }
      
      if(theWhere == "") {
        alert('The "where" field is required');
       return; 
      }
      
      if(theWhat != "") {
        theURL = "/q/" + theWhere + "-restaurants/" + theWhat + "/";
      } else {
        theURL = "/q/" + theWhere + "-restaurants/";
      }
      
      window.location = theURL;
    }
    </script>

